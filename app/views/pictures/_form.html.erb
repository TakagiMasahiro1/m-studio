    <%= form_with(model: picture, local: true) do |form| %>
      <%= form.file_field :image %>
      <%= form.text_field :account, placeholder: "ここにSNSのリンクをを追加してもっと作品を見てもらいましょう！"%>
      <%= form.text_area :text, placeholder: "ここに制作ソフトや制作期間を入力してください", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>